# Databricks Configuration (Direct Connection)
DATABRICKS_SERVER_HOSTNAME=your-databricks-hostname.cloud.databricks.com
DATABRICKS_HTTP_PATH=/sql/1.0/warehouses/your-warehouse-id
DATABRICKS_ACCESS_TOKEN=your-databricks-access-token

# Delta Sharing Configuration (Alternative to Direct Connection)
DELTA_SHARE_PROFILE_PATH=./config.share
DELTA_SHARE_NAME=your_share_name
DELTA_SCHEMA_NAME=your_schema_name
DELTA_TABLE_NAME=subscription_transactions

# Oracle ATP Configuration
ORACLE_USER=ADMIN
ORACLE_PASSWORD=your-oracle-atp-password
ORACLE_DSN=your_tns_name_high
ORACLE_WALLET_LOCATION=./Wallet_YourATP
ORACLE_WALLET_PASSWORD=
